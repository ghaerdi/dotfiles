{
  "suggest.echodocSupport": true,
  "suggest.maxCompleteItemCount": 20,
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "python",
    "php",
    "markdown",
    "javascript",
    "typescript",
    "json",
    "jsonc"
  ],
  "eslint.filetypes": [
    "javascript",
    "typescript",
    "typescriptreact",
    "javascriptreact",
    "typescript.tsx"
  ],
  "eslint.options": {
    "env": {
      "browser": true,
      "node": true
    },
    "diagnostic.errorSign": "✕",
    "diagnostic.warningSign": "⚠",
    "diagnostic.infoSign": "•"
  }
}
